<odoo>
    <record id="view_order_form_inherit_channel" model="ir.ui.view">
        <field name="name">sale.order.form.inherit.channel</field>
        <field name="model">sale.order</field>
        <field name="inherit_id" ref="sale.view_order_form"/>
        <field name="arch" type="xml">
            <xpath expr="//sheet/group" position="inside">
                <field name="canal_id" options="{'no_create': False}" required="1"/>
            </xpath>
        </field>
    </record>
</odoo>
